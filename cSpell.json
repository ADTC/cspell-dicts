{
    "version": "0.1",
    "language": "en",
    "words": [
        "ALLCAPS",
        "appname",
        "configstore",
        "django",
        "django's",
        "issuehunt",
        "liberapay",
        "otechie",
        "patreon",
        "streetsidesoftware"
    ],
    "flagWords": [],
    "maxNumberOfProblems": 1000,
    "ignorePaths": [
        ".vscode/**",
        "**/.git/**",
        "**/*.aff",
        "**/*.dic",
        "**/node_modules/**"
    ],
    "import": [
        "./packages/es_ES/cspell-ext.json",
        "./packages/fr_FR/cspell-ext.json",
        "./packages/da_DK/cspell-ext.json"
    ],
    "overrides": [
        {
            "filename": "**/nl_NL/**",
            "language": "en,nl"
        },
        {
            "filename": "**/fr_FR/**/*.txt",
            "language": "en,fr"
        },
        {
            "filename": "**/da_DK/**",
            "language": "en,da"
        },
        {
            "filename": "**/de_DE/**",
            "language": "en,de"
        },
        {
            "filename": "**/en_GB/**",
            "language": "en_GB"
        },
        {
            "filename": "**/ru_RU/samples/**",
            "language": "ru,en"
        },
        {
            "filename": "**/sv/**",
            "language": "en,sv"
        }
    ]
}